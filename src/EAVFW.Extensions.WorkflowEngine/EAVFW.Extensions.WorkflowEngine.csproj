<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netcoreapp3.1;net5.0;net6.0</TargetFrameworks>
		 
		<Nullable>disable</Nullable>
		<Title>EAVFW.Extensions.WorkflowEngine</Title>
		<Authors>Poul Kjeldager</Authors>
		<Description>Extension for WorkFlow Engine on Hangfire for EAVFW Busines Application Framework</Description>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/EAVFW/EAVFW.Extensions.WorkflowEngine</RepositoryUrl>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="manifest.extensions.json" />
	</ItemGroup>

	<ItemGroup>
		<None Include="..\..\README.md" Link="README.md" Pack="true" PackagePath="\" />
		<None Include="manifest.extensions.json" Pack="true" PackagePath="eavfw/manifest/" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Delegate.WorkflowEngine.Hangfire" Version="3.0.0-dev.5" />
		<PackageReference Include="DotNetDevOps.Extensions.EAVFramework" Version="1.0.153" />
		<PackageReference Include="EAVFW.Extensions.Documents" Version="1.0.0" />
		<PackageReference Include="Hangfire.AspNetCore" Version="1.7.30" />
		<PackageReference Include="Hangfire.Core" Version="1.7.30" />
		<PackageReference Include="Hangfire.SqlServer" Version="1.7.30" />
	</ItemGroup>

</Project>